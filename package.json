    "@types/testing-library__jest-dom": "^5.9.5",
    "gh-pages": "^5.0.0",
    "husky": "^4.3.8",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
