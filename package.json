  },
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.0.0",
    "css-vendor": "^2.0.5",
    "hoist-non-react-statics": "^3.2.1",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "moize": "^6.0.0",
    "nano-css": "^5.0.0",
    "prop-types": "^15.7.2"
  },
  "overrides": {
